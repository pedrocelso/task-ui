{
  "name": "task-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "@material-ui/icons": "^4.4.3",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "fluture": "^11.0.1",
    "jquery": "^3.5.0",
    "jsonwebtoken": "^9.0.0",
    "moment-timezone": "^0.5.25",
    "node-sass": "^4.13.1",
    "popper.js": "^1.15.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jsonschema-form": "^1.5.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.1.2",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "request-promise": "^4.2.4",
    "sinon": "^7.1.0",
    "typescript": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/App.{js,jsx,ts,tsx}",
      "!src/index.{js,jsx,ts,tsx}",
      "!src/setupTests.{js,jsx,ts,tsx}",
      "!src/App-store.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/bluebird-global": "^3.5.11",
    "@types/bootstrap": "^4.3.0",
    "@types/dotenv": "^6.1.1",
